/** Tab Widgets **/
QTabWidget {
  border: none;
}

QTabWidget::pane {
  background: transparent;
  border: none;
  top: -1px;
}

QTabBar::tab {
  background: #31363b;
  border-radius: 4px;
  min-height: 40px;
  min-width: 80px;
  font: 10pt "Lato Heavy";
  color: #f0f0f0;
}

QTabBar::tab:selected {
  background: #555a5f;
  color: #eff0f1;
}

/** Progress Bars **/
QProgressBar {
  background: #626568;
  border-radius: 4px;
  font: 10pt "Lato Heavy";
  color: #f0f0f0;
}

QProgressBar::chunk:horizontal {
  background: qlineargradient(x1: 0.0, y1: 0.0, x2: 1.0, y2: 0.0, stop: 0.0 rgba(48, 48, 48, 100%), stop: 1.0 rgba(0, 170, 255, 100%));
  border-radius: 4px;
  margin: 2px;
}

StatusAdjustmentBar QToolButton {
  width: 40px;
  height: 40px;
}

/** Menus **/
QMenu {
  background-color: #31363b;
  color: #f0f0f0;
}

QMenu::item:selected {
  background: #555a5f;
  color: #eff0f1;
}

/** Groupboxes **/
QGroupBox {
  background: transparent;
  border: 1px solid #76797c;
  border-radius: 4px;
  margin-top: 1ex;
  font: 10pt "Lato Heavy";
}

QGroupBox::Title {
  subcontrol-origin: margin;
  subcontrol-position: top-center;
  padding: 4px 4px;
  color: #f0f0f0;
}

QGroupBox::indicator {
  width: 20px;
  height: 20px;
}

QGroupBox::indicator:unchecked {
  image: url("qtdragon/style_rc/checkbox_unchecked.png");
}

QGroupBox::indicator:checked {
  image: url("qtdragon/style_rc/checkbox_checked.png");
}

/** Dialogs **/
QDialog {
  background-color: #31363b;
}

QDialog QPushButton {
  width: 100px;
  height: 50px;
}

/** Listviews **/
QListView,
QTreeView {
  background: #31363b;
  selection-background-color: transparent;
  qproperty-alternatingRowColors: false;
  font: 10pt "Lato Heavy";
  color: #f0f0f0;
}

QListView::item:hover,
QTreeView::item:hover,
QListView::item:selected,
QTreeView::item:selected {
  border: #727578;
}

QTreeView::item:pressed {
  background: #555a5f;
  color: #eff0f1;
}

/** Sliders **/
QSlider::groove:horizontal {
  background: #1d2023;
  height: 12px;
  border: 1px solid black;
  border-radius: 4px;
}

QSlider::groove:vertical {
  background: #1d2023;
  width: 12px;
  border: 1px solid black;
  border-radius: 4px;
}

QSlider::sub-page:horizontal {
  background: qlineargradient(x1: 0.0, y1: 0.0, x2: 1.0, y2: 0.0, stop: 0.0 rgba(48, 48, 48, 100%), stop: 1.0 rgba(0, 170, 255, 100%));
  border: 1px solid #626568;
  border-radius: 4px;
}

QSlider::handle:horizontal {
  background: #626568;
  border: 1px solid #626568;
  border-radius: 4px;
  width: 24px;
  margin: -8px 0px;
}

QSlider::handle:vertical {
  background: #31363b;
  border: 1px solid #626568;
  border-radius: 4px;
  height: 24px;
  margin: 0px -8px;
}

/** Labels **/
