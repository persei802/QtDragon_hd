/** Main Window **/
QMainWindow {
  background: #19232D;
}

/** Checkboxes **/
QCheckBox {
  spacing: 10px;
  margin-bottom: 2px;
  font: 10pt "Lato Heavy";
  color: #DFE1E2;
}

QCheckBox:disabled {
  color: #788D9C;
}

QCheckBox::indicator {
  width: 16px;
  height: 16px;
}

QCheckBox::indicator:unchecked {
  image: url("qtdragon/dark_rc/checkbox_unchecked.png");
}

QCheckBox::indicator:checked {
  image: url("qtdragon/dark_rc/checkbox_checked.png");
}

/** Comboboxes **/
QComboBox {
  background: #293544;
  border-radius: 4px;
  min-width: 100px;
  padding: 1px 0px 1px 8px;
  font: 10pt "Lato Heavy";
  color: #DFE1E2;
}

QComboBox::drop-down {
  subcontrol-origin: margin;
  subcontrol-position: top right;
  border: none;
  width: 40px;
}

QComboBox::item {
  background-color: #293544;
}

QComboBox::item:selected {
  background: #54687A;
}

QComboBox::down-arrow {
  color: white;
  image: url("qtdragon/dark_rc/arrow_down.png");
}

/** Dialogs **/
QDialog {
  background-color: #293544;
}

QDialog QPushButton {
  width: 100px;
  height: 50px;
}

/** Groupboxes **/
QGroupBox {
  background: transparent;
  border: 1px solid #788D9C;
  border-radius: 4px;
  margin-top: 1ex;
  font: 10pt "Lato Heavy";
}

QGroupBox::Title {
  subcontrol-origin: margin;
  subcontrol-position: top-center;
